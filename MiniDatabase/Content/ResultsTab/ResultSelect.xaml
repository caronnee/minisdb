<parentTab:RecordableTab x:Class="MiniDatabase.Content.ResultsTab.ResultSelect"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:parentTab="clr-namespace:MiniDatabase.Content.ResultsTab"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <StackPanel>
        <TextBlock x:Name="Conditions"></TextBlock>
        <StackPanel>
            <ListBox x:Name="attname"></ListBox>
            <ListBox x:Name="op">
                <ListBoxItem>And</ListBoxItem>
                <ListBoxItem>Or</ListBoxItem>
                <ListBoxItem>Not</ListBoxItem>
            </ListBox>
            <DockPanel x:Name="Dummyholder"></DockPanel>
        </StackPanel>
        <Button>Select</Button>
    </StackPanel>
</parentTab:RecordableTab>
